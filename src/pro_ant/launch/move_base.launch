<launch>    
  <!--- Run AMCL -->
  <!--<include file="$(find amcl)/examples/amcl_diff.launch" /> -->
  <include file="$(find turtlebot_navigation)/launch/includes/amcl.launch.xml"/>

  <!-- Define your move_base node -->
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find pro_ant)/costmap_common_params.yaml" command="load" ns="global_costmap"/>
    <rosparam file="$(find pro_ant)/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find pro_ant)/local_costmap_params.yaml"  command="load" />
    <rosparam file="$(find pro_ant)/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find pro_ant)/base_local_planner_params.yaml" command="load" />

    <remap from="map" to="/map" />
  </node>
</launch>